{
  "name": "marinade",
  "description": "MSOE ARM Integrated Architecture Development Environment",
  "homepage": "https://github.com/msoesd-teambeta/marinade",
  "author": {
    "name": "Team Beta"
  },
  "repository": {
    "url": "https://github.com/msoesd-teambeta/marinade"
  },
  "version": "0.0.0",
  "license": "MIT",
  "main": "electron.js",
  "scripts": {
    "start": "npm run live-reload",
    "lint": "tslint -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
    "ngbuild": "rimraf ./dist && ng build",
    "npm-install": "npm --prefix ./dist install ./dist --production",
    "reinstall": "rimraf node_modules dist dist-ng dist-app && npm i",
    "build": "gulp copy --option noreload",
    "package": "rimraf ./dist-app && npm run ngbuild && npm run build && npm run npm-install && node \"./node_modules/electron-packager/cli.js\" dist Marinade --icon=src/assets/icons/marinade.icns --out=dist-app --overwrite",
    "live-reload": "rimraf ./dist && rimraf ./dist-ng && gulp watch",
    "test": "rimraf ./dist && rimraf ./dist-ng && npm run build && npm run npm-install && ng test",
    "test-ci": "rimraf ./dist && rimraf ./dist-ng && npm run build && npm run npm-install && ng test --code-coverage --single-run --sourcemap=false",
    "docker:start": "docker start sentry-redis sentry-postgres sentry-web-01 sentry-worker-01 sentry-cron",
    "docker:stop": "docker stop sentry-redis sentry-postgres sentry-web-01 sentry-worker-01 sentry-cron"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.0",
    "@covalent/code-editor": "^1.0.0",
    "@covalent/core": "^1.0.0",
    "@covalent/dynamic-forms": "^1.0.0",
    "@covalent/highlight": "^1.0.0",
    "@covalent/http": "^1.0.0",
    "@covalent/markdown": "^1.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "raven-js": "^3.24.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.102",
    "@types/selenium-webdriver": "^3.0.8",
    "chokidar": "^2.0.2",
    "codecov": "^3.0.0",
    "codelyzer": "^4.0.1",
    "electron": "^1.8.4",
    "electron-connect": "^0.6.3",
    "electron-packager": "^11.1.0",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-electron": "^5.2.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.7.2",
    "protractor": "~5.1.2",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.1",
    "semver": "^5.5.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "~2.5.3"
  }
}
